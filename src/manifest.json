{
  "name": "carbon-tab",
  "description": "A Vue.js web extension",
  "version": null,
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab/newtab.html"
  },
  "browser_action": {
    "default_title": "carbon-tab"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  }
}
